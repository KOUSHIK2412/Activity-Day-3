2. Program to print the prime factors of a given integer

#include <stdio.h>

int main()
{
    int n,i,flag=0,s,r,a[100];
    printf("Enter the number\n");
    scanf("%d",&n);
    printf("Prime Factors are : ");
    for(i=2;i<n;i++)
    {
        if(n%i==0)
        {
          for(s=2;s<=i-1;s++)
          {
              if(i%s==0)
               flag=1;
              break;
          }
          if(flag=0)
            printf("%d ",i);
    
        }
    }
    
}
