1. Program to print the factors of a given integer

#include <stdio.h>

int main()
{
    int n,i,j=0,flag,a[100];
    printf("Enter the number\n");
    scanf("%d",&n);
    for(i=2;i<=n;i++)
    {
        if(n%i==0)
          a[j++]=i;
    }
    flag=j;
    printf("Factors are : 1  ");
    for(i=0;i<flag;i++)
      printf("%d  ",a[i]);

    return 0;
}
